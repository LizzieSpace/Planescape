{
  "schemaVersion": 1,
  "id": "planescape",
  "version": "${version}",
  "name": "Planescape",
  "description": "Tool for dimension generation and management.",
  "authors": ["Niko Hori K."],
  "contact": {
    "email": "horikomaru@gmail.com",
    "homepage": "https://github.com/LizzieSpace/Planescape",
    "sources": "https://github.com/LizzieSpace/Planescape",
    "issues": "https://github.com/LizzieSpace/Planescape/issues"
  },
  "license": "MIT",
  "icon": "assets/planescape/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": ["de.horiko.planescape.client.PlanescapeClient"],
    "main": ["de.horiko.planescape.Planescape"]
  },
  "mixins": [
    "planescape.mixins.json", {
      "config": "planescape.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}"
  },
  "custom": {
    "modmenu": {
      "links": {
          "modmenu.wiki": ""
      }
    }
  }
}
